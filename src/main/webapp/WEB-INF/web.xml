<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>aliased-jsp</servlet-name>
        <jsp-file>/home.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>aliased-jsp</servlet-name>
        <url-pattern>/no-leaks</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>login-jsp</servlet-name>
        <jsp-file>/index.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>login-jsp</servlet-name>
        <url-pattern>/abcdf</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>animal1-jsp</servlet-name>
        <jsp-file>/Animal_I.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>animal1-jsp</servlet-name>
        <url-pattern>/secret1</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>animal2-jsp</servlet-name>
        <jsp-file>/Animal_II.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>animal2-jsp</servlet-name>
        <url-pattern>/secret2</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>animal3-jsp</servlet-name>
        <jsp-file>/Animal_III.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>animal3-jsp</servlet-name>
        <url-pattern>/secret3</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>animal4-jsp</servlet-name>
        <jsp-file>/Animal_IV.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>animal4-jsp</servlet-name>
        <url-pattern>/secret4</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>2</session-timeout>
    </session-config>
</web-app>


